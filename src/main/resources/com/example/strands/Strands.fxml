<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.strands.StrandsController">
   <children>
      <Button fx:id="addChar" layoutX="31.0" layoutY="40.0" mnemonicParsing="false" onAction="#addChar" text="ADD" />
      <Button fx:id="nextRow" layoutX="29.0" layoutY="86.0" mnemonicParsing="false" onAction="#addList" text="NEXT ROW" />
      <Button fx:id="doneBtn" layoutX="27.0" layoutY="174.0" mnemonicParsing="false" onAction="#done" text="DONE" />
      <Button fx:id="removeChar" layoutX="28.0" layoutY="130.0" mnemonicParsing="false" onAction="#removeChar" text="BACK" />
      <TextField fx:id="input" layoutX="200.0" layoutY="40.0" />
      <GridPane fx:id="rowGrid" layoutX="200.0" layoutY="73.0" prefHeight="25.0" prefWidth="94.0">
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
        </rowConstraints>
      </GridPane>
      <Button fx:id="prevBtn" layoutX="16.0" layoutY="229.0" mnemonicParsing="false" onAction="#prev" text="PREVIOUS" visible="false" />
      <Button fx:id="nextBtn" layoutX="25.0" layoutY="264.0" mnemonicParsing="false" onAction="#next" text="NEXT" visible="false" />
      <Label fx:id="theWord" layoutX="261.0" layoutY="44.0" text="Label" visible="false" />
      <GridPane fx:id="boardGrid" layoutX="200.0" layoutY="111.0">
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
         </rowConstraints>
      </GridPane>
   </children>
</AnchorPane>
